@article{emek_soylu_deep-learning-based_2023,
	title = {Deep-Learning-Based Approaches for Semantic Segmentation of Natural Scene Images: A Review},
	volume = {12},
	issn = {2079-9292},
	url = {https://www.mdpi.com/2079-9292/12/12/2730},
